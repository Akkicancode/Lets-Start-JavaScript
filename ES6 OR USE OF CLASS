
//CLASS
 class information {
constructor(firstname , lastname , age , dob)  {
    this.firstname= firstname;
    this.lastname= lastname;
    this.age= age;
    this.dob=  dob;
}

getFullName(){
   return ` ${this.firstname} ${this.lastname}`;
}
  
}
//instantiate usin new keyword
const info = new information ('akki', 'shady' , 19, '18/10/1999');
const info1 = new information ('vikki', 'malhotra' , 24 , '12/11/1996');
console.log(info.getFullName());
